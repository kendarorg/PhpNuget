    <connectionStrings>
      <add name="phpnuget" connectionString="Server=127.0.0.1;Database=phpnuget;Uid=phpnuget;Pwd=password;"/>
    </connectionStrings>
    <appSettings>
      <add key="NugetUrl" value="http://127.0.0.1:8020/pnm" />
